@model RandomDataWeb.Models.RandomDataViewModel

    @{
        ViewBag.Title = "Generator losowych danych";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

<h2>Generator losowych danych</h2>




<table>
    <tr>
        <td>Imie:</td>	<td><input type="text" name="first-name" value="@Model.FirstName.Name" readonly><br></td>
    </tr>
    <tr>
        <td>Nazwisko:</td>	<td><input type="text" name="last-name" value="@Model.LastName.NameMale" readonly><br></td>
    </tr>    
    <tr>
        <td>Ulica:</td>	<td><input type="text" name="street" value="@Model.Street.Name" readonly><br></td>
    </tr>    
    <tr>
        <td>Numer domu:</td>	<td><input type="text" name="number" value="@Model.Number" readonly><br></td>
    </tr>    
    <tr>
        <td>Miasto:</td>	<td><input type="text" name="city" value="@Model.City.Name" readonly><br></td>
    </tr>    
    <tr>
        <td>Kod pocztowy:</td>	<td><input type="text" name="zip-code" value="@Model.ZipCode" readonly><br></td>
    </tr>      <tr>
        <td>Wojewodztwo:</td>	<td><input type="text" name="state" value="@Model.State.Name" readonly><br></td>
    </tr>    
</table>